
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Data Brand</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Input Data Brand</li>
            </ol>
          </div>
        </div>
      </div>
    </section>


    <!-- Main content -->
    <section class="content">
        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <div class="mt-4">
                <a href="add_brand" class="d-block">
                <div class="btn btn-primary btn-lg btn-sm">
                  <i href="" class="fas fa-plus"></i>
                  TAMBAH DATA BRAND
                </div>
                </a>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table id="example1" class="table table-bordered table-striped">
                  <thead>
                    <tr>
                    <th>No</th>
                    <th>Nama Produk</th>
                    <th>Tanggal Input</th>
                    <th>Aksi</th>
                    </tr>

                  </thead>

                  <tbody>
                    <% if(data.length){%>
                      <% for(i=0 ; i < data.length; i++){%>
                    <tr>
                      <td><%=data[i].brand_id%></td>
                      <td><%=data[i].name%></td>
                      <td><%=data[i].date_input%></td>
                      <td>
                        <a class="btn btn-info btn-sm" href="/express/admin/edit_brand/<%- data[i].brand_id %>">
                          <i class="fas fa-pencil-alt">
                          </i>
                          Edit
                      </a>
                      <a class="btn btn-danger btn-sm" href="#">
                          <i class="fas fa-trash">
                          </i>
                          Delete
                      </a>
                      </td>
                    </tr>
                    <% } } %>
                    
                  </tbody>

                  <tfoot>
                   
                  </tfoot>
                </table>
      </div>
      <!-- /.card-body -->
      </div>
      <!-- /.card -->
      </div>
      <!-- /.col -->
      </div>
      <!-- /.row -->
      </section>
      <!-- /.content -->
